<h2>{%trans%}Monthly means{%endtrans%}</h2>
<table class="table">
    <tr>
        <th></th>
        <th class="text-right">{%trans%}Last month{%endtrans%}</th>
        <th class="text-right">{%trans%}Last 3 months{%endtrans%}</th>
        <th class="text-right">{%trans%}Last 6 months{%endtrans%}</th>
        <th class="text-right">{{ "now"|date("Y") }}</th>
        <th class="text-right">{{ "first day of last year"|date("Y") }}</th>        
    </tr>    
    {% for child in children %}
    <tr>
        <td><a href="{{ path((routePrefix~'see'), { (entityName~'_id'): child.id }) }}">{{child}}</a></td>
        <td class="text-right">{% include "::amountFormatting.html.twig" with {amount: monthlyMeans.children[child.id].one} %}</td>
        <td class="text-right">{% include "::amountFormatting.html.twig" with {amount: monthlyMeans.children[child.id].three} %}</td>
        <td class="text-right">{% include "::amountFormatting.html.twig" with {amount: monthlyMeans.children[child.id].six} %}</td>
        <td class="text-right">{% include "::amountFormatting.html.twig" with {amount: monthlyMeans.children[child.id].thisYear} %}</td>
        <td class="text-right">{% include "::amountFormatting.html.twig" with {amount: monthlyMeans.children[child.id].lastYear} %}</td>
    </tr>
    {% endfor %}
    <tr>
        <th>{{entity}}</th>
        <th class="text-right">{% include "::amountFormatting.html.twig" with {amount: monthlyMeans.current.one} %}</th>
        <th class="text-right">{% include "::amountFormatting.html.twig" with {amount: monthlyMeans.current.three} %}</th>
        <th class="text-right">{% include "::amountFormatting.html.twig" with {amount: monthlyMeans.current.six} %}</th>
        <th class="text-right">{% include "::amountFormatting.html.twig" with {amount: monthlyMeans.current.thisYear} %}</th>
        <th class="text-right">{% include "::amountFormatting.html.twig" with {amount: monthlyMeans.current.lastYear} %}</th>
    </tr>
</table>